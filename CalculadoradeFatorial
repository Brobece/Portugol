algoritmo "FatorialFuncao"
var
   N, F: Inteiro

Funcao Fatorial(X: Inteiro): Inteiro
Var
   C, R: Inteiro
Inicio
     R <- 1
     Para C <- 1 ate X faca
        R <- R * C
     Fimpara
     Retorne R
Fimfuncao

inicio
   escreval ("Digite um numero: ")
   Leia (N)
   F <- Fatorial(N)
   escreval ("O fatorial de ", N, " e:", F)
fimalgoritmo

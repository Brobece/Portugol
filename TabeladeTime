algoritmo "TabeladeTime"
var
   times: vetor[1..3] de Caractere
   i,j: inteiro
inicio
   Para i <- 1 ate 3 faca
      Escreval ("Digite o nome do time participante:" )
      Leia(times[i])
   FimPara
   LimpaTela
   Para i <- 1 ate 3 faca
      para j <- 1 ate 3 faca
        se times[i] <> times[j] entao
          escreval ((times[i]),"   x   ", times[j])
        FimSe
      fimpara
   fimpara
fimalgoritmo

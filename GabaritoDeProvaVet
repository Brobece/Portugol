algoritmo "GabaritoDeProvaVet"
var
   gabarito, aluno: vetor[1..5] de caractere
   resultado, nota: vetor[1..3] de inteiro
   i,p,n, cont : inteiro
   resposta: caractere
inicio
// Dados da prova
   Para i <- 1 ate 5 faca
     Escreval ("Questao ", i,  ": ")
     Leia (gabarito[i])
   FimPara
   
// Dados dos alunos
   Para p <- 1 ate 3 faca
      LimpaTela
      cont <- 0
      Escreval ("Nome do aluno: ")
      Leia (aluno[p])
      Para i <- 1 ate 5 faca
         Escreval ("Resposta da questÃ£o", i, ": ")
         Leia (resposta)
         se (gabarito[i] = resposta) entao
            cont <- cont + 2
         Fimse
      FimPara
      nota[p] <- cont

   FimPara
   para p <- 1 ate 3 faca
   Escreval ("a nota de ", aluno[p], " foi: ", nota[p])
   fimpara
fimalgoritmo
